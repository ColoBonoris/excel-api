{
  "name": "excel-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ColoBonoris/excel-api.git",
  "author": "Nicol√°s Bonoris <nicolas.bonoris194136@alumnos.info.unlp.edu.ar>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts && open http://localhost:3000/api-docs",
    "worker": "ts-node src/infrastructure/workers/uploadWorker.ts",
    "rabbitmq:start": "docker-compose up -d rabbitmq",
    "rabbitmq:stop": "docker-compose stop rabbitmq",
    "rabbitmq:restart": "docker-compose restart rabbitmq",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:restart": "docker-compose down && docker-compose up -d",
    "db:clean": "docker-compose down -v",
    "redis:start": "docker-compose up -d redis",
    "redis:stop": "docker-compose stop redis",
    "redis:restart": "docker-compose restart redis",
    "redis:clean": "docker-compose rm -f redis && docker volume prune -f",
    "test:generate-excel": "ts-node ./src/tests/utils/generateTestExcel.ts",
    "test:generate-mapping": "ts-node ./src/tests/utils/generateTestMapping.ts",
    "test:generate-all": "yarn test:generate-excel && yarn test:generate-mapping"
  },
  "dependencies": {
    "amqplib": "^0.10.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "mongoose": "^8.12.1",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/xlsx": "^0.0.36",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "prettier": "^3.5.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2",
    "wait-on": "^8.0.3"
  },
  "packageManager": "yarn@4.7.0+sha512.5a0afa1d4c1d844b3447ee3319633797bcd6385d9a44be07993ae52ff4facabccafb4af5dcd1c2f9a94ac113e5e9ff56f6130431905884414229e284e37bb7c9"
}
